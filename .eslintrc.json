{
    "extends": ["airbnb-base", "eslint:recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            { "singleQuote": true },
            {
                "printWidth": 120
            }
        ]
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "settings": {
        "react": {
            "version": "999.999.999"
        }
    }
}
